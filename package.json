{
  "name": "www.autorem.kz",
  "version": "1.0.0",
  "description": "This is a website for the car service www.autorem.kz",
  "main": "index.html",
  "scripts": {
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "watch:all": "parallelshell 'npm run watch:scss' 'npm run lite'",
    "clean": "rimraf dist",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts",
    "copyrobot": "copyfiles robots.txt dist/",
    "copysitemap": "copyfiles sitemap.xml dist/",
    "imagemin": "copyfiles img/* dist/ copyfiles *.png dist/ copyfiles *.ico dist/",
    "usemin": "usemin menu.html -d dist --htmlmin -o dist/menu.html && usemin contactus.html -d dist --htmlmin -o dist/contactus.html && usemin aboutus.html -d dist --htmlmin -o dist/aboutus.html && usemin index.html -d dist --htmlmin -o dist/index.html  && usemin translator.html -d dist --htmlmin -o dist/translator.html",
    "build": "npm run clean && npm run imagemin && npm run copyfonts && npm run copyrobot && npm run copysitemap && npm run usemin"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.24.1",
    "cssmin": "^0.4.3",
    "del": "^3.0.0",
    "grunt": "^1.0.3",
    "grunt-browser-sync": "^2.2.0",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^2.2.1",
    "grunt-contrib-imagemin": "^2.0.1",
    "grunt-contrib-uglify": "^3.4.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-filerev": "^2.3.1",
    "grunt-sass": "^2.1.0",
    "grunt-usemin": "^3.1.1",
    "gulp": "^4.0.0",
    "gulp-clean-css": "^3.10.0",
    "gulp-flatmap": "^1.0.2",
    "gulp-htmlmin": "^4.0.0",
    "gulp-imagemin": "^4.1.0",
    "gulp-rev": "^8.1.1",
    "gulp-sass": "^3.2.1",
    "gulp-uglify": "^3.0.1",
    "gulp-usemin": "^0.3.29",
    "htmlmin": "0.0.7",
    "jit-grunt": "^0.10.0",
    "lite-server": "^2.4.0",
    "node-sass": "^4.9.4",
    "onchange": "^3.3.0",
    "parallelshell": "^3.0.2",
    "rimraf": "^2.6.2",
    "time-grunt": "^1.4.0",
    "uglify-js": "^3.4.9",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.5.1"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "copyfiles": "^2.1.0",
    "cryptiles": "^4.1.2",
    "font-awesome": "^4.7.0",
    "hoek": "^5.0.4",
    "imagemin": "^6.1.0",
    "jquery": "^3.3.1",
    "sshpk": "^1.15.1",
    "tether": "^1.4.5"
  }
}
